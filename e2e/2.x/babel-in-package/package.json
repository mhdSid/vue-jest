{
  "name": "vue2-babel-in-package",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest --no-cache test.js"
  },
  "dependencies": {
    "source-map": "0.5.6",
    "vue": "^2.7.7",
    "vue-template-compiler": "^2.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@vue/test-utils": "^1.1.0",
    "@vue/vue2-jest": "^29.0.0",
    "coffeescript": "^2.0.3",
    "jest": "29.x",
    "jest-environment-jsdom": "29.x",
    "typescript": "^2.5.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.vue$": "@vue/vue2-jest"
    }
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "env"
        ]
      }
    }
  }
}
